                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++",
                "CMAKE_CXX_STANDARD": "17",
                "TPH_CI": "ON",
                "TPH_BuildTests": "ON",
                "TPH_BuildExamples": "OFF",
