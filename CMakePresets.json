{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "gcc",
            "displayName": "GCC",
            "description": "GCC",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++",
                "TPH_CI": "ON"
            },
            "environment": {},
            "vendor": {}
        },
        {
            "name": "gcc-ci",
            "displayName": "gcc-ci",
            "description": "GCC CI",
            "binaryDir": "${sourceDir}/build",
            "generator": "Ninja",
